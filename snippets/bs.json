	"Block (hph).": {
		"prefix": "bs.block.hph",
		"body": [
			"{",
			"  \"$schema\": \"https://app.blockstudio.dev/schema\",",
			"  \"name\": \"blockstudio/$1\",",
			"  \"title\": \"${2:Blockname}\",",
			"  \"category\": \"hph-blocks\",",
			"  \"icon\": \"${3|admin-site,align-center,align-left,align-none,align-right,analytics,archive,arrow-down,arrow-down-alt,arrow-down-alt2,arrow-left,arrow-left-alt,arrow-left-alt2,arrow-right,arrow-right-alt,arrow-right-alt2,arrow-up,arrow-up-alt,arrow-up-alt2,art,awards,backup,book,book-alt,book-alt2,building,businessman,calendar,camera,carrot,category,chart-area,chart-bar,chart-line,chart-pie,clipboard,clock,cloud,cloud-saved,cloud-upload,cloudy,dashboard,desktop,dismiss,download,edit,ellipsis,email,exerpt-view,external,facebook,facebook-alt,feedback,flag,format-aside,format-audio,format-chat,format-gallery,format-image,format-links,format-quote,format-status,format-video,forms,googleplus,grid-view,groups,heart,help,hidden,images-alt,images-alt2,info,instagram,key,layout,leftright,lightbulb,list-view,location,location-alt,location-alt2,lock,marker,media-archive,media-audio,media-code,media-default,media-document,media-interactive,media-spreadsheet,media-text,media-video,megaphone,menu,menu-alt,menu-alt2,microphone,migrate,minus,money,move,nametag,networking,no,no-alt,palmtree,paperclip,performance,phone,playlist-audio,playlist-video,plus,portfolio,post-status,post-trash,pressthis,products,randomize,redo,share,shield,share-alt,share-alt2,slides,smartphone,smiley,sort,sort-down,sort-up,sos,star-filled,star-half,star-outline,store,table-col-after,table-col-before,table-col-delete,table-row-after,table-row-before,table-row-delete,table,tablet,tag,tagcloud,testimonial,text,thumbs-down,thumbs-up,tickets-alt,tickets,tips,trash,twitter,undo,update,upload,vault,video-alt,video-alt2,video-alt3,visibility,welcome-add-page,welcome-comments,welcome-edit-page,welcome-learn-more,welcome-view-site,welcome-widgets-menus,wordpress,wordpress-alt,yes|}\",",
			"  \"description\": \"${4:Blockstudio block.}\",",
			"  \"keywords\": [",
			"    \"block\", \"hph\"",
			"  ],",
			"  \"version\": \"1.0.0\",",
			"  \"textdomain\": \"hph\",",
			"  \"blockstudio\": {",
			"    \"attributes\": [",
			"      $5",
			"    ]",
			"  }",
			"}"
		],
		"description": "Block boilerplate for homepage-helden."
	},
	"Text Field": {
		"prefix": "bs.text",
		"body": [
			"{",
			"    \"id\": \"${1:text}\",",
			"    \"type\": \"text\",",
			"    \"label\": \"${2:Text}\",",
			"    \"default\": \"${3:Text}\",",
			"    \"fallback\": \"${3:Text}\",",
			"    \"placeholder\": \"${3:Text}\"",
			"},"
		],
		"description": "Renders a single line text input."
	},
	"Number Field": {
		"prefix": "bs.number",
		"body": [
			"{",
			"    \"id\": \"${1:number}\",",
			"    \"type\": \"number\",",
			"    \"label\": \"${2:Number}\",",
			"    \"default\": ${3:0},",
			"    \"fallback\": ${3:0},",
			"    \"placeholder\": \"${4:Number}\"",
			"},"
		],
		"description": "Renders a number input"
	},
	"Checkbox Field": {
		"prefix": "bs.checkbox",
		"body": [
			"{",
			"    \"id\": \"${1:checkbox}\",",
			"    \"type\": \"checkbox\",",
			"    \"label\": \"${2:Checkbox}\",",
			"    \"options\": [",
			"        {",
			"            \"value\": \"option-1\",",
			"            \"label\": \"Option 1\"",
			"        },",
			"        {",
			"            \"value\": \"option-2\",",
			"            \"label\": \"Option 2\"",
			"        }",
			"    ]",
			"},"
		],
		"description": "Renders a set of checkbox inputs."
	},
	"WYSIWYG Field": {
		"prefix": "bs.wysiwyg",
		"body": [
			"{",
			"    \"id\": \"${1:wysiwyg}\",",
			"    \"type\": \"wysiwyg\",",
			"    \"label\": \"${2:wysiwyg}\",",
			"    \"toolbar\": {",
			"        \"tags\": {",
			"            \"headings\": [",
			"                1,",
			"                2,",
			"                3,",
			"                4,",
			"                5,",
			"                6",
			"            ]",
			"        },",
			"        \"formats\": {",
			"            \"bold\": true,",
			"            \"italic\": true,",
			"            \"underline\": true,",
			"            \"strikethrough\": true,",
			"            \"code\": true,",
			"            \"orderedList\": true,",
			"            \"unorderedList\": true,",
			"            \"textAlign\": {",
			"                \"alignments\": [",
			"                    \"left\",",
			"                    \"center\",",
			"                    \"right\",",
			"                    \"justify\"",
			"                ]",
			"            },",
			"            \"link\": {",
			"                \"opensInNewTab\": true,",
			"                \"attributes\": {",
			"                    \"rel\": \"nofollow\"",
			"                }",
			"            }",
			"        }",
			"    }",
			"},"
		],
		"description": "Renders a WYSIWYG editor."
	},
	"Conditions": {
		"prefix": "bs.conditions",
		"body": ["\"conditions\": [", " \t[$1]", "]"],
		"description": "Creates the conditions property."
	},
	"Condition.": {
		"prefix": "bs.condition",
		"body": [
			"{",
			"  \"${1|id,type|}\": \"$2\",",
			"  \"operator\": \"${3|==,!=,includes,!includes,empty,!empty,<,>,<=,>=|}\",",
			"  \"value\": \"$4\"",
			"},"
		],
		"description": "Creates a condition."
	},
	"Files Field": {
		"prefix": "bs.files",
		"body": [
			"{",
			"    \"id\": \"${1:files}\",",
			"    \"type\": \"files\",",
			"    \"label\": \"${2:Files}\",",
			"    \"multiple\": ${3|true,false|},",
			"    \"returnFormat\": ${4|\"id\",\"url\",\"object\"|},",
			"    \"allowedTypes\": ${5:[\"image/*\"]}",
			"},"
		],
		"description": "Renders a button to the media library. Picked items can be reordered inline."
	},
	"Group Field.": {
		"prefix": "bs.group",
		"body": [
			"{",
			"    \"id\": \"${1:group}\",",
			"    \"type\": \"group\",",
			"    \"title\": \"${2:Group}\",",
			"    \"initialOpen\": ${3|true,false|},",
			"    \"attributes\": [",
			"        $4",
			"    ]",
			"},"
		],
		"description": "Renders multiple fields in an (optionally) collapsible container."
	},
	"Link Field.": {
		"prefix": "bs.link",
		"body": [
			"{",
			"    \"id\": \"${1:link}\",",
			"    \"type\": \"link\",",
			"    \"label\": \"${2:Link}\",",
			"    \"opensInNewTab\": ${3|true,false|}",
			"},"
		],
		"description": "Renders a link control to choose internal or external links."
	},
	"Select Field.": {
		"prefix": "bs.select",
		"body": [
			"{",
			"  \"id\": \"${1:select}\",",
			"  \"type\": \"select\",",
			"  \"label\": \"${2:Select}\",",
			"  \"multiple\": ${3|true,false|},",
			"  \"allowNull\": ${4|true,false|},",
			"  \"default\": \"${5:option-1}\",",
			"  \"options\": [",
			"    {",
			"      \"value\": \"${6:option-1}\",",
			"      \"label\": \"${7:Option 1}\"",
			"    },",
			"    {",
			"      \"value\": \"${8:option-2}\",",
			"      \"label\": \"${9:Option 2}\"",
			"    }",
			"  ]",
			"},"
		],
		"description": "Renders a select input with support for single or multiple selections."
	},
	"Toggle Field.": {
		"prefix": "bs.toggle",
		"body": [
			"{",
			"  \"id\": \"${1:toggle}\",",
			"  \"type\": \"toggle\",",
			"  \"label\": \"${2:Toggle}\",",
			"  \"default\": ${3|true,false|}",
			"},"
		],
		"description": "Renders a true/false toggle."
	},
	"Radio Field": {
		"prefix": "bs.radio",
		"body": [
			"{",
			"    \"id\": \"${1:radio}\",",
			"    \"type\": \"radio\",",
			"    \"label\": \"${2:Radio}\",",
			"    \"options\": [",
			"        {",
			"            \"value\": \"${3:option-1}\",",
			"            \"label\": \"${4:Option 1}\"",
			"        },",
			"        {",
			"            \"value\": \"${5:option-2}\",",
			"            \"label\": \"${6:Option 2}\"",
			"        }",
			"    ]",
			"},"
		],
		"description": "Renders a set of radio inputs."
	},
	"Icon Field": {
		"prefix": "bs.icon",
		"body": [
			"{",
			"    \"id\": \"${1:icon}\",",
			"    \"type\": \"icon\",",
			"    \"label\": \"${2:Icon}\"",
			"},"
		],
		"description": "Renders an SVG icon from an icon set."
	},
	"Token Field": {
		"prefix": "bs.token",
		"body": [
			"{",
			"    \"id\": \"${1:token}\",",
			"    \"type\": \"token\",",
			"    \"label\": \"${2:Token}\",",
			"    \"options\": [",
			"        \"${3:option-1}\",",
			"        \"${4:option-2}\"",
			"    ]",
			"},"
		],
		"description": "Renders a field similar to the “to” field in Mail on OS X. Tokens can be entered by typing them or selecting them from a list of suggested tokens."
	},
	"Textarea Field": {
		"prefix": "bs.textarea",
		"body": [
			"{",
			"    \"id\": \"${1:textarea}\",",
			"    \"type\": \"textarea\",",
			"    \"label\": \"${2:Textarea}\",",
			"    \"default\": \"${3:Textarea}\",",
			"    \"fallback\": \"${3:Textarea}\",",
			"    \"placeholder\": \"${3:Textarea}\"",
			"},"
		],
		"description": "Renders a textarea input."
	},
	"Repeater Field": {
		"prefix": "bs.repeater",
		"body": [
			"{",
			"    \"id\": \"${1:repeater}\",",
			"    \"type\": \"repeater\",",
			"    \"label\": \"${2:Repeater}\",",
			"    \"min\": ${3:1},",
			"    \"max\": ${4:2},",
			"    \"textButton\": \"${5:Add repeater 2}\",",
			"    \"attributes\": [",
			"        $6",
			"    ]",
			"},"
		],
		"description": "Renders a set of fields that can be repeated."
	},
	"Color Field": {
		"prefix": "bs.color",
		"body": [
			"{",
			"    \"id\": \"${1:color}\",",
			"    \"type\": \"color\",",
			"    \"label\": \"${2:Color}\",",
			"    \"options\": [",
			"        {",
			"            \"name\": \"${3:red}\",",
			"            \"value\": \"${4:#f00}\",",
			"            \"slug\": \"${5:red}\"",
			"        },",
			"        {",
			"            \"name\": \"${6:white}\",",
			"            \"value\": \"${7:#fff}\",",
			"            \"slug\": \"${8:white}\"",
			"        }",
			"    ]",
			"},"
		],
		"description": "Renders a color palette and color picker."
	},
	"Date Field": {
		"prefix": "bs.date",
		"body": [
			"{",
			"    \"id\": \"${1:date}\",",
			"    \"type\": \"date\",",
			"    \"label\": \"${2:Date}\"",
			"},"
		],
		"description": "Renders a date picker."
	},
	"Tabs Field": {
		"prefix": "bs.tabs",
		"body": [
			"{",
			"    \"type\": \"tabs\",",
			"    \"tabs\": [",
			"        {",
			"            \"title\": \"${1:Tab 1}\",",
			"            \"attributes\": [",
			"                $2",
			"            ]",
			"        }",
			"    ]",
			"},"
		],
		"description": "Renders a tabbed interface for grouping fields."
	},
	"Range Field": {
		"prefix": "bs.range",
		"body": [
			"{",
			"    \"id\": \"${1:range}\",",
			"    \"type\": \"range\",",
			"    \"label\": \"${2:Radio}\"",
			"},"
		],
		"description": "Renders a range input to set a numerical value between two points."
	},
	"Gradient Field": {
		"prefix": "bs.gradient",
		"body": [
			"{",
			"    \"id\": \"${1:gradient}\",",
			"    \"type\": \"gradient\",",
			"    \"label\": \"${2:Gradient}\",",
			"    \"options\": [",
			"        {",
			"            \"name\": \"${3:JShine}\",",
			"            \"value\": \"${4:linear-gradient(135deg,#12c2e9 0%,#c471ed 50%,#f64f59 100%)}\",",
			"            \"slug\": \"${5:jshine}\"",
			"        },",
			"        {",
			"            \"name\": \"${6:Moonlit Asteroid}\",",
			"            \"value\": \"${7:linear-gradient(135deg,#0F2027 0%, #203A43 0%, #2c5364 100%)}\",",
			"            \"slug\": \"${8:moonlit-asteroid}\"",
			"        }",
			"    ]",
			"},"
		],
		"description": "Renders a gradient palette and gradient picker."
	},
	"Datetime Field": {
		"prefix": "bs.datetime",
		"body": [
			"{",
			"    \"id\": \"${1:datetime}\",",
			"    \"type\": \"datetime\",",
			"    \"label\": \"${2:Datetime}\"",
			"},"
		],
		"description": "Renders a date and time picker."
	},
	"Populate Field": {
		"prefix": "bs.populate",
		"body": [
			"\"populate\": {",
			"    \"type\": \"query\",",
			"    \"query\": \"${1:posts}\",",
			"    \"arguments\": {",
			"        \"post_type\": \"${2:jobs}\",",
			"        \"numberposts\": \"${3:20}\"",
			"    }",
			"}"
		],
		"description": "Defines a population query."
	},
	"Unit Field": {
		"prefix": "bs.unit",
		"body": [
			"{",
			"    \"id\": \"${1:unit}\",",
			"    \"type\": \"unit\",",
			"    \"label\": \"${2:Unit}\",",
			"    \"default\": \"${2:1rem}\",",
			"    \"options\": [",
			"        {",
			"            \"value\": \"${3:px}\",",
			"            \"label\": \"${4:px}\",",
			"            \"default\": ${5:0}",
			"        },",
			"        {",
			"            \"value\": \"${3:%}\",",
			"            \"label\": \"${4:%}\",",
			"            \"default\": ${5:10}",
			"        }",
			"    ]",
			"},"
		],
		"description": "Renders a number input with a unit dropdown."
	}
}
